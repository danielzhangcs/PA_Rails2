<% provide(:title, 'All users') %>
<h1>All users</h1>

<div class="">
<%= will_paginate :previous_label =>"<<", :next_label => ">>" %>
</div>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, size: 50 %>
      <%= link_to user.name, user %>
    </li>
  <% end %>
</ul>

<%= will_paginate  :previous_label =>"<<", :next_label => ">>"%>
