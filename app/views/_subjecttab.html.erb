
<div class="tab-pane fade" id="subject" role="tabpanel" aria-labelledby="subject-tab">
  <table class="table">
    <thead>
      <tr>
       <th scope="col">Id</th>
        <th scope="col">Name</th>
        <th scope="col">Abbreviation</th>

        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @subjects.each do |subject| %>
        <tr scope="row">
            <td><%= subject.id %></td>
          <td><%= subject.name %></td>
          <td><%= subject.abbreviation %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>